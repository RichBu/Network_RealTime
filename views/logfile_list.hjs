        <main class="container container-fluid containerStyle">
			<div style="max-height: 1000px;">
                <div class="row rowStyle row-click" data-row={{movieID}}>
					<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
						<h5>
						<table>
  							<tr class="row row-hover">
    						<th class="tdNorm">Number</th>
    						<th class="tdNorm">Uploaded</th>
							<th class="tdNorm">File name   </th>
  							</tr>
							{{#outputObj}}
								<tr class="row row-hover">
									<td class="tdNorm">{{files_log_id}}</td>
									<td class="tdNorm">{{time_of_upload_str}}</td>
									<td class="tdNorm">{{filename_str}}</td>
								</tr>
							{{/outputObj}}
